lg shadow-md\">\r\n            <h2 className=\"text-2xl font-bold mb-6 text-emerald-700\">Volunteer with Us</h2>\r\n            <p className=\"mb-6 text-gray-600\">Join our team of dedicated volunteers and make a real difference in the lives of children and our environment. Fill out the form below to get started!</p>\r\n\r\n            <Form {...form}>\r\n              <form onSubmit={form.handleSubmit(onSubmit)} className=\"space-y-6\">\r\n                <FormField\r\n                  control={form.control}\r\n                  name=\"name\"\r\n                  render={({ field }) => (\r\n                    <FormItem>\r\n                      <FormLabel>Name</FormLabel>\r\n                      <FormControl>\r\n                        <Input placeholder=\"Your full name\" {...field} />\r\n                      </FormControl>\r\n                      <FormMessage />\r\n                    </FormItem>\r\n                  )}\r\n                />\r\n                <FormField\r\n                  control={form.control}\r\n                  name=\"email\"\r\n                  render={({ field }) => (\r\n                    <FormItem>\r\n                      <FormLabel>Email</FormLabel>\r\n                      <FormControl>\r\n                        <Input type=\"email\" placeholder=\"Your email address\" {...field} />\r\n                      </FormControl>\r\n                      <FormMessage />\r\n                    </FormItem>\r\n                  )}\r\n                />\r\n                <FormField\r\n                  control={form.control}\r\n                  name=\"phone\"\r\n                  render={({ field }) => (\r\n                    <FormItem>\r\n                      <FormLabel>Phone</FormLabel>\r\n                      <FormControl>\r\n                        <Input type=\"tel\" placeholder=\"Your phone number\" {...field} />\r\n                      </FormControl>\r\n                      <FormMessage />\r\n                    </FormItem>